[Unit]
Description=Moonraker
After=network.target klipper.service
Wants=network.target klipper.service

[Service]
Type=simple
User=prusa
WorkingDirectory=/home/prusa/klipper_stack/moonraker
ExecStart=/home/prusa/klipper_stack/venv/bin/python /home/prusa/klipper_stack/moonraker/moonraker/moonraker.py -c /home/prusa/printer_data/config/moonraker.conf -l /home/prusa/printer_data/logs/moonraker.log
Restart=on-failure
RestartSec=2

[Install]
WantedBy=multi-user.target
